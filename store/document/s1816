&lt;script language=&quot;JavaScript&quot;&gt;
    var delta=0.15
    var collection;
    function floaters() {
        this.items    = [];
        this.addItem    = function(id,x,y,content)
                  {
                    document.write('&lt;DIV id='+id+' style=&quot;Z-INDEX: 10; POSITION: absolute;  width:80px; height:60px;left:'+(typeof(x)=='string'?eval(x):x)+';top:'+(typeof(y)=='string'?eval(y):y)+'&quot;&gt;'+content+'&lt;/DIV&gt;');
                    
                    var newItem                = {};
                    newItem.object            = document.getElementById(id);
                    newItem.x                = x;
                    newItem.y                = y; 
                    this.items[this.items.length]        = newItem;
                  }
        this.play    = function()
                  {
                    collection                = this.items
                    setInterval('play()',10);
                  }
        }
        function play()
        {
            if(screen.width&lt;=800)
            {
                for(var i=0;i&lt;collection.length;i++)
                {
                    collection[i].object.style.display    = 'none';
                }
                return;
            }
            for(var i=0;i&lt;collection.length;i++)
            {
                var followObj        = collection[i].object;
                var followObj_x        = (typeof(collection[i].x)=='string'?eval(collection[i].x):collection[i].x);
                var followObj_y        = (typeof(collection[i].y)=='string'?eval(collection[i].y):collection[i].y); 
                if(followObj.offsetLeft!=(document.body.scrollLeft+followObj_x)) {
                    var dx=(document.body.scrollLeft+followObj_x-followObj.offsetLeft)*delta;
                    dx=(dx&gt;0?1:-1)*Math.ceil(Math.abs(dx));
                    followObj.style.left=followObj.offsetLeft+dx;
                    } 
                if(followObj.offsetTop!=(document.body.scrollTop+followObj_y)) {
                    var dy=(document.body.scrollTop+followObj_y-followObj.offsetTop)*delta;
                    dy=(dy&gt;0?1:-1)*Math.ceil(Math.abs(dy));
                    followObj.style.top=followObj.offsetTop+dy;
                    }
                followObj.style.display    = '';
            }
        }    
        
    var theFloaters        = new floaters();
    theFloaters.addItem('followDiv1','document.body.clientWidth-106',80,'&lt;br&gt;&lt;a href=http://www.cau.edu.cn/cyc/cyc/ad/index.html target=_blank&gt;&lt;img  src=&quot;images/ad.jpg&quot;  border=0&gt;&lt;/a&gt;');
    theFloaters.addItem('followDiv2',6,80,'&lt;br&gt;&lt;a href=http://www.cau.edu.cn/cyc/cyc/ad/index.html target=_blank&gt;&lt;img src=&quot;images/ad.jpg&quot;  border=0&gt;&lt;/a&gt;'); 
    theFloaters.play();    
&lt;/script&gt; menu /menu 企 业 名 称 负责人 电 话 手 机 传 真 经营内容 备 注 北京中农大地科技发展有限公司 傅泽田 62328899-3301 62328899-3988 经营管理 有限 北京东方畅想建筑设计有限公司 南国良 82838866-2004 13601079662 82838866-2202 建筑设计 股份制 北京双电电子电力技术公司 杨仁刚 62736745 13901063528 62736742 电子电力 国有 正大涿州肉鸡发展中心 马守东 03123792637 13603241328 03123792183 种鸡 国有 北农(海利)涿州种衣剂有限公司 丁明 03123792298 13607316621 03123792900 种衣剂 国有 北京市富通环境工程有限公司 马承伟 62329109 13910717814 62736917-117 湿帘纸 国有 北京市先飞农业工程高技术公司 杨培岭 62310037 13321162786 62736911 节水技术 国有 北京欣中农油泵油嘴维修有限公司 王惠菁 62736574 13501382995 62398501 维修服务 股份制 北农大新技术开发总公司 刘志平 62732908 13601304985 62731056 国有 中国农业大学出版社 汪春林 62731203 13501023345 62732620 出版图书 国有 北京震亚预混饲料厂 呙玉明 62815567 13901029899 62733900 饲 料 国有 北京圆西文化服务中心 黄兴建 62732737 13693350938 62732716 打字、印刷 国有 北京思农玉米育种开发中心 苏胜宝 62733057 13701226224 62731023 玉米良种 国有 中农大生物技术股份有限公司 高恩复 82375379 82375728-220 生产、销售兽药、农药 股份制 中农大网络发展公司 顾培德 62966818 13601837660 62966655-500 网络教育 股份制 北京龙科方舟饲料有限公司 李德发 62731456 62829803-8001 有限 北农大动物科技有限责任公司 张庆才 62731102 13701177627 62731090 动物饲料 股份制 中农大康科技开发有限公司 李建生 82893682 13671337475 82781958-804 玉米新品种选育 股份制 北京我爱我爱动物医院有限公司 汪明 87375252-216 13601293795 87371999-555 诊治宠物疾病 股份制 金码大酒店(国际) 李世盛 62328899-3996 13701190461 62328899-3901 酒店管理 国有 北京中农大科技企业孵化器有限公司 胡金有 62737370 13601363645 62732266 企业孵化 股份制 北京中农大科技开发有限责任公司 邱晓松 62736884 13911114029 62736884 股份制 中农金旺（北京）农业工程技术有限公司 张德林 82388788 13501260817 82388788 农业设施温室工程 股份制 中国农业大学校办产业办公室 
